    "        \n",
    "        display(self.main_container)\n",
    "    \n",
    "    def create_main_components(self):\n",
    "        \"\"\"Create all UI components\"\"\"\n",
    "        # Authentication components\n",
    "        self.hf_token_input = widgets.Password(\n",
